
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /W/Motivational-Wordpress-Website/wordpress/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /W/Motivational-Wordpress-Website/wordpress/index.php [L]
</IfModule>

# END WordPress